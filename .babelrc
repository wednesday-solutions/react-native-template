{
    "plugins": [
      [
        "module-resolver",
        {
          "cwd": "babelrc",
          "root": ["./src"],
          "extensions": [".js", ".ios.js", ".android.js"],
          "alias": {
            "@assets": "./app/assets",
            "@components": "./app/components",
            "@atoms": "./app/components/atoms",
            "@molecules": "./app/components/molecules",
            "@organisms": "./app/components/organisms",
            "@config": "./app/config",
            "@navigators": "./app/navigators",
            "@scenes": "./app/scenes",
            "@services": "./app/services",
            "@themes": "./app/themes",
            "@utils": "./app/utils"
          }
        }
      ]
    ]
  }
